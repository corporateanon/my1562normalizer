{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Gobify",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/gobify"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Normalize",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/tasks/normalize.js",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Load addresses",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/tasks/load-addresses.js",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Process addresses",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/tasks/process-addresses.js",
            "console": "integratedTerminal"
        }
    ]
}
